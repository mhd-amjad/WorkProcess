{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DaprSidekick": {
    "Sidecar": {
      "AppId": "zeebe-worker",
      "AppPort": 5000,
      "DaprGrpcPort": 53500,
      "DaprHttpPort": 5001,
      "ComponentsDirectory": "./components"
    }
  },
  "MyActorConfig": {
    "actorIdleTimeout": 5,
    "actorScanInterval": 2,
    "drainOngoingCallTimeout": 10,
    "drainRebalancedActors": true
  }
}